void move(int x,int y)
{
	printf(x);
        printf("-->",y);
	return;
}

void hanoi(int n,int one,int two,int three)
{
	if(n==1)
	{
	 move(one,three);
	}
        else
	{
	  hanoi(n-1,one,three,two);
	  move(one,three);
	  hanoi(n-1,two,one,three);
	}
	return;
}

void main()
{
	int m;
	scanf(m);
	hanoi(m,1,2,3);
}