const one=1,two=2,three=3;
void move1(int f,int t)
{ 
  printf(f);
  printf(" move to ",t); 
  return ;
 
} 
void moven(int m,int a,int b,int c) 
{
  if(m==1) 
    move1(a,c); 
  else 
  { 
    moven(m-1,a,c,b); 
    move1(a,c); 
    moven(m-1,b,a,c); 
  } 

	return ;
} 
void main() 
{ 
 int n; 
 printf("please input the numbers of disks: "); 
 scanf(n); 
 printf("the step of moving disks: ",n); 
 moven(n,one,two,three); 
} 